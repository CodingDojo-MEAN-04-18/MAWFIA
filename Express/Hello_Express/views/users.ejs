<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <title>Hello Express</title>
        <!--[if lt IE 9]>
        <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    </head>
    <body>
        <h2>Here are all the users:</h2>
        <% for (var x in users) { %>
            <h3>Name: <%= users[x].name %></h3>
            <h4>Email: <%= users[x].email %></h4>
            <hr>
        <% } %>
    </body>
</html>
